{
  "name": "@jtjs/react",
  "version": "2.0.0",
  "license": "MIT",
  "description": "A component library for React applications.",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/jtjs_react.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/jtjs_react.js"
      }
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:types",
    "build:types": "tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "docgen": "npx react-docgen -o ./react-docgen-output.json ./lib/**/*.tsx --pretty && npx ts-node ./scripts/gen-md-from-react-docgen.script.ts ./react-docgen-output.json ./libs/react/README && npx rimraf ./react-docgen-output.json"
  },
  "peerDependencies": {
    "@jtjs/browser": "^2.0.0",
    "@jtjs/view": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@react-docgen/cli": "^2.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "happy-dom": "^15.7.4",
    "react-docgen": "^7.0.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.1.1"
  }
}
